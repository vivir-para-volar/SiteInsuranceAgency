@model InsuranceAgency.Models.Security.ChangeProfile

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Modify Profile</title>
</head>
<body>
    <h1>Register</h1>
    @using (Html.BeginForm("ChangeProfile", "Account", FormMethod.Post))
    {
        @Html.AntiForgeryToken();
        <table>
            <tr>
                <td>@Html.LabelFor(m => m.FullName)</td>
                <td>@Html.TextBoxFor(m => m.FullName)</td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.BirthDate)</td>
                <td>@Html.TextBoxFor(m => m.BirthDate)</td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.PhoneNumber)</td>
                <td>@Html.TextBoxFor(m => m.PhoneNumber)</td>
            </tr>
            <tr>
                <td><input type="submit" value="Change" /></td>
            </tr>
        </table>
    }
    @ViewBag.Message
    @Html.ValidationSummary()
</body>
</html>
